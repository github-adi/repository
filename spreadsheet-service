//get data out of spreadsheet\\
function getDate() {
  
  var ss = SpreadsheetApp.getActive();
  var sheet = ss.getActiveSheet();
  var data = sheet.getDataRange();
  var values = data.getValues();
  
  Logger.log(values);
  
  
}


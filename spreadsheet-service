//get active data\\
function getData4() {
  
  var ss = SpreadsheetApp.getActiveSpreadsheet();
  var sheet1 = ss.getSheetByName("Sheet1");
  
  var active = sheet1.getDataRange().getValues();
    
  Logger.log(active);
  
  
}


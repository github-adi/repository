            Function IEBinary_getByteAt(strBinary, iOffset)
               IEBinary_getByteAt = AscB(MidB(strBinary,iOffset+1,1))
            End Function
            Function IEBinary_getLength(strBinary)
               IEBinary_getLength = LenB(strBinary)
            End Function
       </script>


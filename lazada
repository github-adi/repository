$(".custlivechatbutton").click(function(e){
  e.preventDefault();
  var se = document.createElement('script'); se.type = 'text/javascript'; se.async = true;
  se.src = 'http://storage.googleapis.com/code.snapengage.com/js/72b6e844-1e7c-40fe-9b99-b7d4d1aa8d04.js';
  var done = false;
  se.onload = se.onreadystatechange = function() {
    if (!done&&(!this.readyState||this.readyState==='loaded'||this.readyState==='complete')) {
      done = true;
      SnapEngage.getAgentStatusAsync(function (online) {
        if(online) {
          $("#SnapABug_bImg").trigger("click");
        }else{
          alert("Mohon maaf, fasilitas livechat tersedia di jam kerja  09.00am - 08.00pm (Hari Kerja) dan 09.00am - 05.00pm (Hari Libur)")
          SnapEngage.hideButton();
        }
      });
    }
  };
  var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(se, s);
});
</script>

<script>
(function(){
    var idStamp = {
        availStamp: {},
        
        init: function(){
            idStamp.initAvailStamp();
            if( $('.product_list').length == 0 && dataLayer[0].pagetype != 'productpage' ) {return false;}

            for(var i in idStamp.availStamp) {
                var curStamp = idStamp.availStamp[i];
                if(curStamp.status) {
                    idStamp.doStamp(curStamp);
                }
            }

            if($('#special_price_box').length){
                if(Number($('#special_price_box').text().replace('RP', '').replace(/\./g, '')) < 1000000){
                    $('#lazada-IP-big').remove();
                }
            } else if($('.product-card').length || $('.unit').length){
                $('.product-card, .unit').find('.product-card__price, .itm-price:first').each(function() {
                    if(Number($(this).text().replace('RP', '').replace(/\./g, '')) < 1000000){
                        $(this).closest('.product-card, .unit').find('.lazada-IP-small').remove();
                    }
                });
            };
            $("#productDetailBadge").height("80px");
        },

        initAvailStamp: function() {
            idStamp.availStamp={
                //'axa': {'status':1, 'selector': '.axa', 'hover': 0, 'classname': 'axa', 'logic': null},
                //'fsh': {'status':1, 'selector': '.fsh', 'hover': 0, 'classname': 'fsh', 'logic': null},
                //'freeshipindo': {'status':1, 'selector': '.freeshipindo', 'hover': 0, 'classname': 'freeshipindo', 'logic': null},
                //'FSI': {'status':1, 'selector': '.FSI', 'hover': 0, 'classname': 'FSI', 'logic': null},
                //'freeshipmedan': {'status':1, 'selector': '.fsmdn', 'hover': 0, 'classname': 'freeshipmedan', 'logic': null},
                //'freesby': {'status':1, 'selector': '.freesby', 'hover': 0, 'classname': 'freesby', 'logic': null},
                //'stdeal': {'status':1, 'selector': '.stdeal', 'hover': 0, 'classname': 'stdeal', 'logic': null},
                //'IP': {'status':1, 'selector': '.IP', 'hover': 0, 'classname': 'IP', 'logic': null},
                //'BLT': {'status':1, 'selector': '.BLT', 'hover': 0, 'classname': 'BLT', 'logic': null},
                'COD': {'status':1, 'selector': '.COD', 'hover': 0, 'classname': 'COD', 'logic': null},
                'exdeal': {'status':1, 'selector': '.exdeal', 'hover': 0, 'classname': 'exdeal', 'logic': null},
                'rmdn': {'status':1, 'selector': '.rmdn', 'hover': 0, 'classname': 'rmdn', 'logic': null}
            }
        },

        doStamp: function(stampData){
            if( $('.product_list').length ) {
                $('.product_list div' + stampData.selector).each(function(){
                    idStamp.addStampToCatalog( [$(this), stampData] );
                })
            } else if(dataLayer[0].pagetype == 'productpage' && $('#productDetailBadge' + stampData.selector).length ) {
                idStamp.addStampToProduct( [$('#productDetailBadge'), stampData] );
            }
        },

        addStampToCatalog: function( data ) {
            wrapper = data[0];
            $stamp = $('<span></span>', {'class': 'lazada-'+data[1].classname+'-small'});
            // Grid View
            wrapper.find('.product-card__img').prepend($stamp);
            // List View
            wrapper.find('.product-card__img').prepend($stamp);
        },

        addStampToProduct: function( data ){
            wrapper = data[0];
            if(data[1].hover) {
                $stamp = $('<a></a>',{'id': 'lazada-'+data[1].classname+'-big', 'href':data[1].lp});
            } else {
                $stamp = $('<span></span>',{'id': 'lazada-'+data[1].classname+'-big'});
            }

            $('#lazada-'+data[1].classname+'-big').remove();
            wrapper.append($stamp);
            if(data[1].logic != null) {
                data[1].logic();
            }
        },

        stampDisclaimerHover: function(){
            var $stampBig    = $('#lazada-bestprice-big').attr('tooltip', 'lazada_lazada_bestprice_terms'),
            
            $tooltipContent  = '<div>';
            $tooltipContent += '<div class="tip left" id="lazada_lazada_bestprice_terms" style="margin-left: 6.5em; margin-top: 0.5em; display: none; width:265px; text-align:left; color: black !important;">';
            $tooltipContent += '<div class="tt-title"><b>Best Price Guarantee</b> </div>';
            $tooltipContent += '<div class="tt-desc">Best Price Guarantee dari Lazada memastikan Anda mendapatkan harga terbaik di semua produk-produk Lazada untuk segala kebutuhan Anda.\<br />Klik <a href="http://www.lazada.co.id/bestpriceguarantee/">disini</a> untuk keterangan selengkapnya.<br/><br/></div>';
            $tooltipContent += '</div>';
            $tooltipContent += '</div>';

            // Add tooltip
            $stampBig.append($tooltipContent);
            $('#lazada-bestprice-big')
                .mouseover(function () {
                    $('#' + $(this).attr('tooltip')).show()
                })
                .mouseleave(function () {
                    $('#' + $(this).attr('tooltip')).hide()
                });

            // Redirect to the disclaimer page
            $stampBig.on('click', function(e){
            e.preventDefault();
            window.location.href = '/bestpriceguarantee/';
            });
        },

        stampExtraHover: function(){
            var $stampBig     = $('#lazada-extradiscount-big').attr('tooltip', 'lazada_lazada_extradiscount_terms'),
            
            $tooltipContent  = '<div>';
            $tooltipContent += '<div class="tip left" id="lazada_lazada_extradiscount_terms" style="margin-left: 6.5em; margin-top: 0.5em; display: none; width:265px; text-align:left; color: black !important;">';
            $tooltipContent += '<div class="tt-title"><b>Extra Discount</b> </div>';
            $tooltipContent += '<div class="tt-desc">Diskon hanya berlaku bagi untuk 1 unit yang bertanda diskon khusus.\<br />Diskon bervariasi untuk setiap produknya dan akan dikenakan pada saat konfirmasi pembayaran.<br/><br/></div>';
            $tooltipContent += '</div>';
            $tooltipContent += '</div>';

            // Add tooltip
            $stampBig.append($tooltipContent);
            $('#lazada-extradiscount-big')
                .mouseover(function () {
                    $('#' + $(this).attr('tooltip')).show()
                })
                .mouseleave(function () {
                    $('#' + $(this).attr('tooltip')).hide()
                });

            // Redirect to the disclaimer page
            $stampBig.on('click', function(e){
            e.preventDefault();
            window.location.href = '/#';
            });
        }
    }

    idStamp.init();
})();
</script>


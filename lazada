  // setCookie('baru_datang',true,1);
// }

  function motogClose(obj) {
    $(obj).parent().remove();
    try{
      localStorage.setItem("motog_closed", (new Date).getTime())
    }catch(e){console.log(e);}
  }

  try {
    var motog_closed = localStorage.getItem("motog_closed");
    if(motog_closed != null) {
      if( (new Date).getDate() == new Date(parseInt(localStorage.getItem("motog_closed"))).getDate() ){
        open_motog = false;
      }
    }
  } catch(e) {console.log(e)}

  if(open_motog){
    $('body').append(html_banner);
  }else{
    $('.stackbanner').remove();
  }  
}


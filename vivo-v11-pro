<script type="text/javascript">
    $(document).ready(function(){
        $( ".videoslider" ).animate({ "left": "0px" }, 2000, 'linear'); 

        $(".closevideoslider").click(function(){
            $( ".videoslider" ).animate({ "left": "-100%" }, 2000, 'linear' , function() {  
                 
                  $(parent.document).find('.floating-slider-ads').remove();
                  $(parent.document).find('.singleimageslider').remove();
                  $(parent.document).find('.popslidevideo').remove();
                  $(parent.document).find('.vidx').remove();
                  $(parent.document).find('.videoslider').remove();
            });
        });

        

        $(".Mutebutton").click( function (){
          if( $("video").prop('muted') ) {
                $("video").prop('muted', false);
                $(this).addClass('UnMutebutton');
          } else {
            $("video").prop('muted', true);
            $(this).removeClass('UnMutebutton');
          }
        });
        
    });

</script>


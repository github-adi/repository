<script type="text/javascript">
function fadeMessage() {
welcomeElement = document.getElementById('welcome');
if (welcomeElement) {
	welcomeElement.style.transitionTimingFunction = "cubic-bezier(0.6, 0.04, 0.98, 0.335)";
	welcomeElement.style.transitionDuration = "10s";	// length of fadeout
	welcomeElement.style.opacity = "0";
}
}
setTimeout(function() { fadeMessage(); },30000);		// delay before fadeout
</script>
<!-- END Message fadeout code -->


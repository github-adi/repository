<script type="text/javascript">
<!-- Start of LazyLoad code -->
if ($('img.lazy').length > 0) {
    $('img.lazy').parent().addClass('img-bg');
    $.lazyLoadXT.onload=function() {
        $(this).parent("a.cloud-zoom, span").addClass("hide-bg");
        $(this).removeClass("lazy-hidden");
        $(this).addClass("lazy-loaded");
    }
    $.lazyLoadXT.updateEvent = 'load orientationchange resize scroll slid.bs.carousel';
}
jQuery(function(){
  jQuery( document ).ajaxStart(function() {
    jQuery( "#ajax-result" ).text("");
  });
  Ladda.bind( 'button', { timeout: 1000 } );
  jQuery( "#open-header-cart-content" ).click(function(e) {
      if (jQuery('#header-cart-content').html() == '') {
          jQuery.ajax({
              url: "https://www.mataharimall.com/checkout/ajax/getheadercart",
              type: 'POST',
              dataType: 'json',
              data: "",
              success: function (data) {
                  if (data.status == 1) {
                      jQuery('#header-cart-content').html(data.html);
                  } else if (data.status == 0) {
                      jQuery('#header-cart-content').html("Cart anda masih kosong.");
                  }
              }
          });
      }
  });
  jQuery("#HeaderSearchCategoryBox").change(function(e) {
    switch(jQuery(this).val()){
      case "":
        cattitle = 'All';
        break;
      case '1845':cattitle = 'Promo';break;case '1':cattitle = 'Handphone & Tablet';break;case '275':cattitle = 'Fashion Pria';break;case '195':cattitle = 'Fashion Wanita';break;case '533':cattitle = 'Jam Tangan & Perhiasan';break;case '1265':cattitle = 'Tas & Koper';break;case '1308':cattitle = 'Olahraga & Outdoor';break;case '43':cattitle = 'TV, Audio & Game';break;case '754':cattitle = 'Peralatan Rumah Tangga';break;case '561':cattitle = 'Elektronik Rumah Tangga';break;case '328':cattitle = 'Kamera & Kamera Video';break;case '357':cattitle = 'Kesehatan & Kecantikan';break;case '1650':cattitle = 'Otomotif';break;case '688':cattitle = 'Komputer & Laptop';break;case '1729':cattitle = 'Groceries';break;case '1463':cattitle = 'Perlengkapan Anak & Bayi';break;case '1761':cattitle = 'Buku & Hobi';break;case '1994':cattitle = 'Voucher';break;    }
    jQuery('#valueHeaderSearchCategoryBox').html(cattitle);
  });
  jQuery("#HeaderSearchCategoryBox").trigger('change');

     // jQuery.ajax({
   //      url: "/checkout/ajax/getcartcount",
   //      type: 'POST',
   //      dataType: 'json',

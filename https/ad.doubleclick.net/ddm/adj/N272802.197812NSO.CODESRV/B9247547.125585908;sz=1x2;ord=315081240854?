  /*disable enter in cart form*/
  jQuery(document).on("keypress", "#coupon-code", function(e) {
      if (e.keyCode == 13) {
        e.preventDefault();
        AddCoupon(this);
        return false;
      }
  });

});
function removeCartItem(store_id, variant_id, step)
{
    jQuery.ajax({
        url: "https://www.mataharimall.com/checkout/ajax/add2cart",
        type: 'POST',
        dataType: 'json',
        data: "store_id=" + store_id + "&variant_id=" + variant_id + "&step=" + step + "&qty=0&mode=updateqty",
        success: function (data) {
            if (data.status == 1) {
                jQuery('#cart-modal-body').html(data.html);
                if (data.cartcount > 0) {
                    jQuery('#header-cart-count').html(data.cartcount).show();
                } else {
                    jQuery('#header-cart-count').html('').hide();
                }
                // if(step=='review')
                // window.location.href='/checkout/review';
                if (typeof $('#header-cart-content') != "undefined") {
                    $('#header-cart-content').html('');
                }
            } else if (data.status == 0) {
                alert(data.msg);
            }
        }
    });
}
function updateCartView(store_id,variant_id,x,step){
  var qty = jQuery(x).val();
  jQuery.ajax({
     url: "https://www.mataharimall.com/checkout/ajax/add2cart",
     type: 'POST',
     dataType: 'json',
     data: "store_id=" + store_id + "&variant_id=" + variant_id + "&qty=" + qty + "&mode=updateqty" + "&step=" + step,
     success: function(data){
        if(data.status==1){
          jQuery('#cart-modal-body').html(data.html);
          if (data.cartcount > 0) {
              jQuery('#header-cart-count').html(data.cartcount).show();
          } else {
              jQuery('#header-cart-count').html('').hide();
          }
          // if(step=='review')
            // window.location.href='/checkout/review';
            if (typeof $('#header-cart-content') != "undefined") {
                $('#header-cart-content').html('');
            }
        }else if(data.status==0){
          alert(data.msg);
        }
     }
  });
}
function AddCoupon(x){
  var l = Ladda.create(x);
  jQuery('#ajax-result-coupon').html('');
  l.start();
  var coupon = jQuery('#coupon-code').val();
  if(coupon==''){
    jQuery('#ajax-result-coupon').html('<div class="alert alert-danger alert-dismissible">Please enter the coupon</div>');
    l.stop();
  }
  jQuery.ajax({
     url: "https://www.mataharimall.com/checkout/ajax/addcoupon",
     type: 'POST',
     dataType: 'json',
     data: "coupon=" + coupon,
     success: function(data){
        if(data.status==1){
          reloadCart();
          l.stop();
        }else if(data.status==0){
          jQuery('#ajax-result-coupon').html('<div class="alert alert-danger alert-dismissible">' + data.msg + '</div>');
          l.stop();
        }
     }
  });
}
function removeCoupon(x){
  var l = Ladda.create(x);
  jQuery('#ajax-result-coupon').html('');
  l.start();
  jQuery.ajax({
     url: "https://www.mataharimall.com/checkout/ajax/removecoupon",
     type: 'POST',
     dataType: 'json',
     data: "1=1",
     success: function(data){
        if(data.status==1){
          reloadCart();
          l.stop();
        }else if(data.status==0){
          jQuery('#ajax-result-coupon').html('<div class="alert alert-danger alert-dismissible">' + data.msg + '</div>');
          l.stop();
        }
     }
  });
}
function reloadCart(){
  jQuery.ajax({
     url: "https://www.mataharimall.com/checkout/ajax/add2cart",
     type: 'POST',
     dataType: 'json',
     data: 'mode=getcart&step=review',
     success: function(data){
        if(data.status==1){
          jQuery('#cart-modal-body').html(data.html);
            if (typeof $('#header-cart-content') != "undefined") {
                $('#header-cart-content').html('');
            }
        }else if(data.status==0){
          alert(data.msg);
        }
     }
  });
}
if (typeof setCookieHasOffers == "function") {
    setCookieHasOffers () ;
}
if (typeof setCookieUtm == "function") {
    setCookieUtm () ;
}
</script>

